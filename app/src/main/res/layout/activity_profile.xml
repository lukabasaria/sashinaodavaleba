        tools:layout_editor_absoluteX="223dp"
        tools:layout_editor_absoluteY="116dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Contacted"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_editor_absoluteX="273dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:text="18"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView6"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="52dp"
        android:text="Reviewed"
        app:layout_constraintBottom_toBottomOf="@+id/textView4"
        app:layout_constraintEnd_toStartOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="48dp"
        android:text="Applied"
        app:layout_constraintEnd_toStartOf="@+id/textView2"
        tools:layout_editor_absoluteY="279dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="88dp"
        android:layout_marginTop="8dp"
        android:text="79"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="28"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_editor_absoluteX="77dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="324dp"
        android:text="Complete Profile"
        android:textColor="@color/black"
        android:textSize="21dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="100dp"
        android:text="Education"
        android:textStyle="normal"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="473dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="100dp"
        android:text="02 Step "
        android:textColor="@color/black"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="520dp" />


    <TextView
        android:id="@+id/textView12"
        android:layout_width="78dp"
        android:layout_height="37dp"
        android:elevation="100dp"
        android:text="@string/left_text"
        android:textColor="@color/black"
        android:textSize="18dp"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="548dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="71dp"
        android:layout_height="63dp"
        android:drawableLeft="@drawable/right_ico_foreground"
        android:elevation="100dp"
        tools:layout_editor_absoluteX="78dp"
        tools:layout_editor_absoluteY="522dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="109dp"
        android:layout_height="169dp"
        android:layout_marginTop="1dp"
        android:backgroundTint="@color/butt_background"
        android:drawableBottom="@drawable/education_ico_foreground"
        android:paddingRight="55dp"
        android:paddingBottom="75dp"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="100dp"
        android:text="Professional"
        android:textStyle="normal"
        tools:layout_editor_absoluteX="211dp"
        tools:layout_editor_absoluteY="473dp" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="100dp"
        android:text="04 Steps "
        android:textColor="@color/black"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="211dp"
        tools:layout_editor_absoluteY="520dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="78dp"
        android:layout_height="37dp"
        android:elevation="100dp"
        android:text="@string/left_text"
        android:textColor="@color/black"
        android:textSize="18dp"
        tools:layout_editor_absoluteX="209dp"
        tools:layout_editor_absoluteY="539dp" />

    <TextView
        android:layout_width="71dp"
        android:layout_height="63dp"
        android:drawableLeft="@drawable/right_ico_foreground"
        android:elevation="100dp"
        tools:layout_editor_absoluteX="226dp"
        tools:layout_editor_absoluteY="522dp" />

    <Button
        android:layout_width="113dp"
        android:layout_height="168dp"
        android:layout_marginStart="40dp"
        android:backgroundTint="@color/bag"
        android:drawableBottom="@drawable/profess_foreground"
        android:paddingRight="55dp"
        android:paddingBottom="75dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView13"
        app:layout_constraintStart_toEndOf="@+id/button3" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:textAlignment="center"
        android:textStyle="bold"
        android:text="@string/buy_text"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="669dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
